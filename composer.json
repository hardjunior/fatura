{
    "name": "hardjunior/hardjunior",
    "type": "sistema",
    "keywords": [
        "sistema",
        "PHP"
    ],
    "description": "Hardjunior sistema de faturas",
    "homepage": "https://github.com/hardjunior/hardjunior",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [{
        "name": "Ivamar JÃºnior",
        "email": "hardjunior1@gmail.com",
        "homepage": "https://github.com/hardjunior",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "matthiasmullie/minify": "^1.3",
        "notihnio/php-request-parser": "1.0.0",
        "hardjunior/mini-frame": "*",
        "filips123/monolog-phpmailer": "^1.0",
        "phpmailer/phpmailer": "^6.1",
        "cboden/ratchet": "^0.4.4",
        "symfony/error-handler": "^6.2",
        "league/plates": "^3.5"
    },
    "require-dev": {
        "filp/whoops": "^2.9",
        "symfony/var-dumper": "^5.4"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Source\\": "source"
        },
        "files": [
            "source/Config.php",
            "source/Helpers.php",
            "source/Rotas.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0,
        "preferred-install": "dist",
        "github-protocols": ["https"],
        "vendor-lib": "vendor"
    }
}